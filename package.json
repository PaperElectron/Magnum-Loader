{
  "name": "magnum-loader",
  "version": "4.2.0",
  "description": "Plugin loader for the Magnum DI dependency injection framework.",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test tap -R spec --cov test/unit/*.js test/integration/**/*.js",
    "test:unit": "NODE_ENV=test tap -R spec --cov test/unit/*.js",
    "test:integration": "NODE_ENV=test tap -R spec --cov test/integration/**/*.js",
    "test:report": "NODE_ENV=test tap -R spec --cov --coverage-report=lcov test/unit/*.js test/integration/**/*.js",
    "test:file": "NODE_ENV=test tap -R spec --cov"
  },
  "keywords": [
    "Dependency Injection",
    "IOC",
    "Framework"
  ],
  "author": "Jim <monstertke@paperelectron.com> (http://paperelectron.com/)",
  "license": "MIT",
  "files": [
    "index.js",
    "lib/",
    "Readme.md"
  ],
  "dependencies": {
    "bluebird": "^3.4.7",
    "chalk": "^1.1.3",
    "debug": "^2.5.2",
    "fs-extra": "^1.0.0",
    "lodash": "^4.17.3",
    "magnum-di": "^2.2.3",
    "magnum-topo": "^0.3.0",
    "parent-require": "^1.0.0"
  },
  "devDependencies": {
    "mockery": "^2.0.0",
    "strip-ansi": "^3.0.1",
    "tap": "^8.0.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PaperElectron/Magnum-Loader.git"
  },
  "bugs": {
    "url": "https://github.com/PaperElectron/Magnum-Loader/issues"
  },
  "homepage": "https://github.com/PaperElectron/Magnum-Loader"
}
